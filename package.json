{
  "name": "gtags-code",
  "displayName": "gtags-code",
  "icon": "icon.png",
  "description": "",
  "publisher": "harikalyan",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Language Packs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hari-sh/gtags-code"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "gtags-code configuration",
      "properties": {
        "gtags-code.globalCmd": {
          "type": "string",
          "default": "global",
          "description": "Path to the global executable"
        }
      }
    },
    "commands": [
      {
        "command": "extension.storeTags",
        "title": "CTags: Store Tags"
      },
      {
        "command": "extension.jumpTag",
        "title": "CTags: Jump To Tag"
      },
      {
        "command": "extension.searchTags",
        "title": "CTags: Search Tags"
      },
      {
        "command": "extension.getReferences",
        "title": "CTags: Get References"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.jumpTag",
          "when": "editorTextFocus",
          "group": "navigation"
        },
        {
          "command": "extension.getReferences",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    },
    "properties": {
      "gtags-code.globalCmd": {
        "type": "string",
        "default": "global",
        "description": "Path to the custom tool executable."
      }
    }
  },
  "scripts": {
    "bundle": "node build.js",
    "vscode:prepublish": "node build.js",
    "package": "vsce package"
  },
  "dependencies": {
    "classic-level": "^3.0.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "esbuild": "^0.25.5"
  }
}
